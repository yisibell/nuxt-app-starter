{
  "name": "nuxt-app-starter",
  "version": "1.3.0",
  "description": "A ssr app starter build by Nuxt(2) + Koa + Vuetify + Typescript + Composition API + sass.",
  "private": true,
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "nuxt",
    "template",
    "pwa",
    "ssr",
    "vuetify",
    "pm2",
    "koa",
    "vue-composition-api",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yisibell/nuxt-app-starter.git"
  },
  "homepage": "https://github.com/yisibell/nuxt-app-starter/blob/main/README.md",
  "scripts": {
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "pm2:docker:start": "pm2-runtime start ecosystem.config.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:docker:start-ft": "pm2-runtime start ecosystem.ft.config.js",
    "pm2:start-ft": "pm2 start ecosystem.ft.config.js",
    "start": "node --max-old-space-size=1536 ./scripts/app.js",
    "start:ft": "node ./scripts/app.js --env=ft",
    "build": "node ./scripts/build.js --theme=default",
    "build:ft": "node ./scripts/build.js --theme=default --env=ft",
    "build:analyze": "node ./scripts/build.js --theme=default --analyze",
    "build:all": "node ./scripts/build.js --all",
    "dev": "node ./scripts/dev.js --theme=default",
    "dev:mobile": "node ./scripts/dev.js --theme=mobile",
    "dev:net": "node ./scripts/dev.js --theme=default --host=0.0.0.0",
    "dev:https": "node ./scripts/dev.js --theme=default --host=0.0.0.0 --port=443 --https=1"
  },
  "dependencies": {
    "@aidol/svg-icon": "^1.2.0",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@nuxtjs/axios": "^5.13.6",
    "cookie": "^0.5.0",
    "core-js": "^3.6.5",
    "defu": "^3.2.2",
    "img-map-area": "^1.0.0",
    "js-cookie": "^3.0.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-sslify": "^5.0.0",
    "nuxt": "~2.15.8",
    "nuxt-persiststate-with-cookie": "^1.2.0",
    "sweetalert2": "^11.4.8",
    "swiper": "^6.4.11",
    "tippy.js": "^6.3.1",
    "ua-parser-js": "^1.0.2",
    "webfontloader": "^1.6.28",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nuxt/types": "~2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/composition-api": "~0.32.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/pwa": "^3.3.3",
    "@nuxtjs/sentry": "^5.0.3",
    "@nuxtjs/vuetify": "^1.12.3",
    "@sentry/cli": "^1.64.2",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.168",
    "@types/uuid": "^8.3.1",
    "@vue/test-utils": "^1.1.0",
    "babel-jest": "^26.5.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^5.0.0",
    "http-proxy-middleware": "^2.0.4",
    "jest": "^26.5.0",
    "koa-connect": "^2.1.0",
    "nuxt-ssr-cache": "^1.5.2",
    "prettier": "^2.1.2",
    "qs": "^6.9.4",
    "redis": "^3.1.2",
    "svg-sprite-loader": "^5.0.0",
    "uuid": "^8.3.2",
    "vue-jest": "^4.0.1",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": ">= 14.x"
  }
}
